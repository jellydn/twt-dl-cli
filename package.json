{
  "name": "twt-dl-cli",
  "version": "0.3.0",
  "description": "The easiest way to download any Twitter video from CLI",
  "keywords": [
    "twt-dl-cli",
    "video",
    "twitter",
    "download"
  ],
  "repository": "https://github.com/jellydn/twt-dl-cli",
  "license": "MIT",
  "author": "Huynh Duc Dung",
  "type": "module",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./dist/cli.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll --env.NODE_ENV=production --minify",
    "cli": "npx tsx ./src/cli.ts",
    "example": "npx tsx ./example/index.ts",
    "lint": "biome check --fix",
    "prepack": "clean-pkg-json",
    "test": "vitest --run",
    "test:tsd": "tsd",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "watch": "pkgroll --watch"
  },
  "nano-staged": {
    "*.{js,ts}": "biome lint --fix",
    "*.{js,ts,css,md}": "prettier --write",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "@types/download": "8.0.5",
    "cheerio": "1.0.0",
    "cleye": "1.3.4",
    "cross-fetch": "4.1.0",
    "download": "8.0.0",
    "ora": "8.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@skypack/package-check": "0.2.2",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "22.14.1",
    "clean-pkg-json": "1.2.1",
    "consola": "3.4.2",
    "husky": "9.1.7",
    "nano-staged": "0.8.0",
    "pkgroll": "2.12.1",
    "prettier": "3.5.3",
    "sort-package-json": "3.0.0",
    "tsd": "0.32.0",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "vite": "6.3.2",
    "vitest": "3.1.1"
  },
  "packageManager": "pnpm@10.8.1",
  "tsd": {
    "directory": "tests"
  }
}
